{"ast":null,"code":"import axios from'axios';import{useEffect,useState}from'react';function Customhokk(url){const[data,setData]=useState([]);const[loading,setLoading]=useState(true);const[error,setError]=useState(null);useEffect(()=>{if(url)setLoading(true);setError(false);axios.get(url).then(response=>{setData(response.data);setLoading(false);setError(false);}).catch(error=>{console.error(error);setLoading(false);setError(true);});},[url]);return{data,loading,error};}export default Customhokk;","map":{"version":3,"names":["axios","useEffect","useState","Customhokk","url","data","setData","loading","setLoading","error","setError","get","then","response","catch","console"],"sources":["D:/jwelshopadmin/src/components/Customhokk.js"],"sourcesContent":["import axios from 'axios'\nimport  { useEffect, useState } from 'react'\n\nfunction Customhokk(url) {\n    const[data, setData] = useState([])\n    const[loading, setLoading] = useState(true)\n    const[error, setError] = useState(null)\n    useEffect(()=>{\n        if(url)\n            setLoading(true)\n        setError(false)\n        axios\n        .get(url)\n        .then((response) => {\n            setData(response.data)\n            setLoading(false)\n            setError(false)\n        })\n        .catch((error) => {\n            console.error(error)\n            setLoading(false)\n            setError(true)\n        })\n    },[url])\n    return {data , loading, error}\n}\n\nexport default Customhokk"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,OAAUC,SAAS,CAAEC,QAAQ,KAAQ,OAAO,CAE5C,QAAS,CAAAC,UAAUA,CAACC,GAAG,CAAE,CACrB,KAAK,CAACC,IAAI,CAAEC,OAAO,CAAC,CAAGJ,QAAQ,CAAC,EAAE,CAAC,CACnC,KAAK,CAACK,OAAO,CAAEC,UAAU,CAAC,CAAGN,QAAQ,CAAC,IAAI,CAAC,CAC3C,KAAK,CAACO,KAAK,CAAEC,QAAQ,CAAC,CAAGR,QAAQ,CAAC,IAAI,CAAC,CACvCD,SAAS,CAAC,IAAI,CACV,GAAGG,GAAG,CACFI,UAAU,CAAC,IAAI,CAAC,CACpBE,QAAQ,CAAC,KAAK,CAAC,CACfV,KAAK,CACJW,GAAG,CAACP,GAAG,CAAC,CACRQ,IAAI,CAAEC,QAAQ,EAAK,CAChBP,OAAO,CAACO,QAAQ,CAACR,IAAI,CAAC,CACtBG,UAAU,